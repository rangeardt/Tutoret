                  <th>mise a jour</th>
                </tr>
                              </thead>
                               <tbody>
{% for salle in liste_salle %} 

                <tr>
                  <td><a href="{{ path('Projet_reseauSalleView', {'salle': salle.nom}) }}" title="Cliquez pour plus de dÃ©tails">{{salle.nom}}</a> </td>
